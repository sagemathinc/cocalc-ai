{
  "name": "@cocalc/lite",
  "version": "0.2.6",
  "description": "CoCalc Lite Standalone Server",
  "exports": {
    "./*": "./dist/*.js",
    "./hub/acp": "./dist/hub/acp/index.js"
  },
  "keywords": [
    "lite",
    "cocalc"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf build dist node_modules tsconfig.tsbuildinfo",
    "build": "pnpm exec tsc --build",
    "test": "pnpm exec jest --forceExit",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores @types/debug,@types/jest,@types/node,ts-node,@cocalc/plus",
    "app": "pnpm exec cocalc-lite",
    "jupyter:bench": "node ./dist/jupyter-benchmark.js",
    "jupyter:bench:browser": "node ./dist/jupyter-browser-benchmark.js",
    "jupyter:bench:scroll": "node ./dist/jupyter-scroll-benchmark.js",
    "jupyter:test:e2e": "playwright test -c playwright.jupyter.config.ts",
    "jupyter:test:e2e:headed": "playwright test -c playwright.jupyter.config.ts --headed"
  },
  "bin": {
    "cocalc-lite": "./bin/start.js"
  },
  "engines": {
    "node": ">=22"
  },
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/ai": "workspace:*",
    "@cocalc/assets": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/chat": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/lite": "workspace:*",
    "@cocalc/project": "workspace:*",
    "@cocalc/static": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.23",
    "@types/lodash": "^4.17.20",
    "@openai/agents": "^0.3.7",
    "ascii-table3": "^1.0.1",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "formidable": "^3.5.4",
    "http-proxy-3": "^1.23.2",
    "lodash": "^4.17.21",
    "selfsigned": "^3.0.1",
    "zod": "^3.25.76"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/test",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.10",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  }
}
